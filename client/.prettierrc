{
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindConfig": "./tailwind.config.js",
	"tailwindPreserveWhitespace": true,
	"trailingComma": "es5",
	"tabWidth": 4,
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"quoteProps": "as-needed",
	"useTabs": false,
	"singleAttributePerLine": true
}
